;AAJW;AAAA;AAAA,uBAMY,KAAK,CAAC,GAAN,CAAU,yCAAV,EACjB;AAAE,kBAAA,OAAO,EAAE;AAAC,qCAAiB,YAAY;AAA9B;AAAX,iBADiB,CANZ;;AAAA;AAML,gBAAA,QANK;AAQT,gBAAA,KAAI,CAAC,aAAL,GAAqB,QAAQ,CAAC,IAA9B;AARS;AAAA;;AAAA;AAAA;AAAA;AAWT,gBAAA,KAAI,CAAC,aAAL;;AAXS;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcf;AAfO;AATI,CAAf","sourcesContent":["<template>\n  <div id=\"home\">\n    <h1>Okta Single-Page App Demo</h1>\n    <div v-if=\"!this.$root.authenticated\">\n      <p>How much caffeine has your developer had today? <router-link role=\"button\" to=\"/login\">Log in to find out!</router-link></p>\n    </div>\n\n    <div v-if=\"this.$root.authenticated\">\n      <p>Welcome, {{claims.name}}!</p>\n      <p>\n        {{this.caffeineLevel}}\n      </p>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport axios from 'axios';\n\nexport default {\n  name: 'home',\n  data: function () {\n    return {\n      claims: '',\n      caffeineLevel: ''\n    }\n  },\n  created () { this.setup() },\n  methods: {\n    async setup () {\n      if (this.$root.authenticated) {\n        this.claims = await this.$auth.getUser()\n        let accessToken = this.$auth.getAccessToken();\n        console.log(`Authorization: Bearer ${accessToken}`);\n        try {\n          let response = await axios.get('http://localhost:8082/howcaffeinatedami',\n              { headers: {'Authorization': 'Bearer ' + accessToken } } );\n          this.caffeineLevel = response.data;\n        }\n        catch (error) {\n          this.caffeineLevel = `${error}`\n        }\n      }\n    }\n  }\n}\n</script>"],"sourceRoot":""}]}