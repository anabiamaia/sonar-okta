ile":"/home/ana-silva1/Documentos/SOLINFTEC/JAVA/spring-boot-spa/okta-app/src/App.vue","sourceRoot":"","sourcesContent":["<template>\n  <div id=\"app2\">\n    <nav>\n      <div>\n        <router-link to=\"/\">\n          Home\n        </router-link>\n        <router-link to=\"/login\" v-if=\"!authenticated\">\n          Login\n        </router-link>\n        <router-link to=\"/profile\" v-if=\"authenticated\" >\n          Profile\n        </router-link>\n        <a v-if=\"authenticated\" v-on:click=\"logout()\">\n          Logout\n        </a>\n      </div>\n    </nav>\n    <div id=\"content\">\n      <router-view/>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'app',\n  data: function () {\n    return { authenticated: false }\n  },\n  async created () {\n    await this.isAuthenticated()\n    this.$auth.authStateManager.subscribe(this.isAuthenticated)\n  },\n  watch: {\n    // Everytime the route changes, check for auth status\n    '$route': 'isAuthenticated'\n  },\n  methods: {\n    async isAuthenticated () {\n      this.authenticated = await this.$auth.isAuthenticated()\n    },\n    async logout () {\n      await this.$auth.signOut()\n    }\n  }\n}\n</script>\n\n<style>\nnav div a { margin-right: 10px }\n#app {\n  width: 800px;\n  margin: 0 auto;\n}\na {\n  text-decoration: underline;\n  cursor: pointer;\n}\n</style>"]}]}